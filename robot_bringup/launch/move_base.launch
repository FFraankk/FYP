<launch>

  <node pkg="move_base" type="move_base" name="move_base" output="screen">
    <rosparam file="$(find your_pkg)/config/costmap_common.yaml" command="load" ns="global_costmap"/>
    <rosparam file="$(find your_pkg)/config/costmap_common.yaml" command="load" ns="local_costmap"/>
    <rosparam file="$(find your_pkg)/config/global_costmap.yaml" command="load"/>
    <rosparam file="$(find your_pkg)/config/local_costmap.yaml" command="load"/>
    <rosparam file="$(find your_pkg)/config/base_local_planner.yaml" command="load"/>
  </node>

</launch>
