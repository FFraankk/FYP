<launch>
  <!-- camera_init -> map (rename/alias) -->
  <node pkg="tf"
        type="static_transform_publisher"
        name="tf_camera_init_to_map"
        args="0 0 0 0 0 0 camera_init map 100" />

  <!-- aft_mapped -> base_link (rename/alias) -->
  <node pkg="tf"
        type="static_transform_publisher"
        name="tf_aft_mapped_to_base_link"
        args="0 0 0 0 0 0 aft_mapped base_link 100" />
</launch>
